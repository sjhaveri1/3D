function [z, dOpt] = regionOpt(regions,gain,slices,half,cxns)
    n = slices/6;
    full = ([half, zeros(122,1)]);
    for i=1:6
        for j=1:length(regions)
            if regions(j) == i
                z(j) = gain*randi([i,n*i],1);
            else
                continue
            end
        end
    end
end